{
	"manifest_version": 2,
	"name": "Tab Notes",
	"description": "Add notes to your Chrome tabs",
	"version": "1.0",
	"background": {
		"scripts": [
			"vendor/md5.min.js",
			"utils.js",
			"background.js"]
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"page_action": {
		"default_icon": {
			"19": "images/notepadEmpty19.png",
			"38": "images/notepadEmpty38.png"
		},
		"default_title": "View Tab Note",
		"default_popup": "note.html"
	}
}
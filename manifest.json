{
	"manifest_version": 2,
	"name": "Tab Notes",
	"description": "Add notes to your Chrome tabs",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"page_action": {
		"default_icon": {
			"19": "images/notepadIcon19.png",
			"38": "images/notepadIcon38.png"
		},
		"default_title": "View Tab Note",
		"default_popup": "note.html"
	}
}